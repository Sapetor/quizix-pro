{
  "name": "quizix-pro",
  "version": "1.0.0",
  "description": "Quizix Pro - Advanced interactive quiz platform for local networks",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build:css": "postcss public/css/main.css -o public/css/main.bundle.css --verbose",
    "build:css:watch": "postcss public/css/main.css -o public/css/main.bundle.css --watch --verbose",
    "cache-bust": "node scripts/cache-bust.js",
    "build": "npm run build:css",
    "build:prod": "npm run build:css && npm run cache-bust",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "cheerio": "^1.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "mammoth": "^1.11.0",
    "multer": "^2.0.1",
    "officeparser": "^6.0.4",
    "pdf-parse": "^1.1.1",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "sharp": "^0.34.5",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/jest": "^30.0.0",
    "autoprefixer": "^10.4.17",
    "cssnano": "^6.0.3",
    "eslint": "^9.28.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "prettier": "^3.7.4"
  },
  "keywords": [
    "quiz",
    "interactive",
    "local",
    "multiplayer",
    "education",
    "latex",
    "images"
  ],
  "author": "",
  "license": "MIT"
}
